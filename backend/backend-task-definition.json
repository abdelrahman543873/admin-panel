{
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/admin-backend",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      },
      "portMappings": [
        {
          "hostPort": 0,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "cpu": 0,
      "image": "ECR_REGISTRY/$BACKEND_ECR_REPOSITORY:$IMAGE_TAG",
      "name": "admin-backend"
    }
  ],
  "memory": "1024",
  "family": "admin-backend",
  "requiresCompatibilities": ["EC2"],
  "cpu": "512",
  "status": "ACTIVE",
  "taskRoleArn": "arn:aws:iam::979925633203:role/test-RootRole-1NYS64WWA6H1M",
  "executionRoleArn": "arn:aws:iam::979925633203:role/test-RootRole-1NYS64WWA6H1M"
}
